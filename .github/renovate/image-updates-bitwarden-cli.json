{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "description": "image-updates | all | bitwarden-cli | configure labels and semantic commit scope",
      "addLabels": [
        "image:bitwarden-cli"
      ],
      "matchManagers": [
        "custom.regex"
      ],
      "matchPackageNames": [
        "bitwarden/clients"
      ],
      "semanticCommitScope": "bitwarden-cli"
    }
  ]
}
