{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "description": "image-updates | all | freeradius-server | configure labels and semantic commit scope",
      "addLabels": [
        "image:freeradius-server"
      ],
      "matchManagers": [
        "custom.regex"
      ],
      "matchPackageNames": [
        "freeradius/freeradius-server"
      ],
      "semanticCommitScope": "freeradius-server"
    }
  ]
}
